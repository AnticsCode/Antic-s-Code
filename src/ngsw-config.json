{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "Antic's Code",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/index.html",
          "/*.css",
          "/*.js",
          "/manifest.webmanifest"
        ],
        "urls": [
          "https://stackpath.bootstrapcdn.com/font-awesome/**",
          "https://fonts.googleapis.com/**"
        ]
      },
      "icons": [
        {
          "src": "assets/img/logo.png",
          "sized": "196x196",
          "type": "image/png",
          "purpose": "maskable"
        }
      ]
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**/**/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "articles-api",
      "urls": ["https://antics-code.herokuapp.com/articles"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 12,
        "maxAge": "4h",
        "timeout": "5s"
      }
    }
  ]
}
