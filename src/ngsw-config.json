{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "Antic's Code",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/assets/icons/favicon.png",
          "/index.html",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://stackpath.bootstrapcdn.com/font-awesome/**",
          "https://fonts.googleapis.com/**"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**/**/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "articles-api",
      "urls": ["https://antics-code.herokuapp.com/articles"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 12,
        "maxAge": "4h",
        "timeout": "5s"
      }
    }
  ]
}
